{
  "extends": [
    "@jakejarvis/eslint-config"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "requireConfigFile": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "ignorePatterns": [
    "*.d.ts",
    "dist/**"
  ]
}
