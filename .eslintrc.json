{
  "extends": [
    "@jakejarvis/eslint-config",
    "plugin:compat/recommended",
    "plugin:import/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "requireConfigFile": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": false
  },
  "ignorePatterns": [
    "*.d.ts",
    "dist/**"
  ]
}
